{
  "cli": {
    "version": ">= 16.16.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "env": {
        "SENTRY_PROPERTIES": "./sentry.properties",
        "SENTRY_AUTH_TOKEN": "$SENTRY_AUTH_TOKEN"
      }
      
    }
  },
  "submit": {
    "production": {}
  }
}
